{
    "name": "gileslloyd/itc-compliance",
    "description": "Recruitment Service",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "akrabat/rka-content-type-renderer": "^1.0",
        "cableandcode/microservice-core": "dev-master",
        "doctrine/migrations": "^2.0@dev",
        "doctrine/orm": "^2.6",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/lumen-framework": "^6.0",
        "league/container": "^2.0",
        "league/tactician": "^1.0",
        "league/tactician-container": "^2.0",
        "monolog/monolog": "^1.23",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store": "^7.5",
        "prooph/event-store-bus-bridge": "^3.3",
        "prooph/pdo-event-store": "^1.11",
        "prooph/pdo-snapshot-store": "^1.5",
        "prooph/service-bus": "^6.2",
        "prooph/snapshotter": "^2.1",
        "ramsey/uuid": "^3.8",
        "symfony/console": "^4.2@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
	"autoload": {
	  "psr-4": {
		"": [
		  "src/Recruitment/Domain/"
		],
		"Application\\": ["src/Recruitment/Application/"],
		"Infrastructure\\": ["src/Recruitment/Infrastructure/"]
	  }
	},
	"repositories": {
	  "0": {
		"type": "git",
		"url": "https://giles_lloyd@bitbucket.org/giles_lloyd/microservice-core.git"
	  }
	},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
